    "@types/jest": "^30.0.0",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^24.6.2",
    "ajv": "^8.17.1",
    "jest": "^30.2.0",
    "ts-jest": "^29.4.4",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.19.1",
    "@xenova/transformers": "^2.17.2",
    "gpt-tokenizer": "^3.2.0",
    "yaml": "^2.8.1",
    "zod": "^3.25.76"
  }
